{
  "name": "backend",
  "version": "1.0.0",
  "description": "Firebase Cloud Functions backend for hack-f1811",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "seed": "ts-node --project scripts/tsconfig.json --transpile-only scripts/seedFirestore.ts",
    "seed:dry-run": "ts-node --project scripts/tsconfig.json --transpile-only scripts/seedFirestore.ts --dry-run",
    "seed:force": "ts-node --project scripts/tsconfig.json --transpile-only scripts/seedFirestore.ts --force",
    "seed:rollback": "ts-node --project scripts/tsconfig.json --transpile-only scripts/seedFirestore.ts --rollback"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/vision": "^4.0.2",
    "@google/generative-ai": "^0.1.3",
    "@types/busboy": "^1.5.4",
    "@types/cors": "^2.8.19",
    "busboy": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^4.5.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "eslint-config-google": "^0.14.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  }
}